<Window x:Class="WPF_QuantConnect.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:WPF_QuantConnect.ViewModels"
        Title="MainWindow" Height="350" Width="525">

    <Window.DataContext>
        <local:MainWindowViewModel />
    </Window.DataContext>
    <Grid>
        <TextBlock Text="{Binding AlgorithmOutput}"
                   TextWrapping="Wrap" 
                   />
    </Grid>
</Window>
